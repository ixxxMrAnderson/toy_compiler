class MY_INT{
    int[] a;
    MY_INT(){
        int i;
        a = new int [10];
        for (i = 0; i < a.size(); ++i){
            a[i] = i;
        }
    }
    void print_int(){
        int i;
        for (i = 0; i < a.size(); ++i){
            print(toString(a[i]));
            print(" ");
        }
        print("\n");
    }
};

int main() {
  MY_INT[] arr = new MY_INT [10];
  int i;
  for (i = 0; i < arr.size(); ++i){
    arr[i].print_int();
  }

  return 0;
}
